{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "addon_version": {
      "type": "string"
    },
    "event": {
      "description": "An event identifier",
      "type": "string"
    },
    "event_context": {
      "description": "A string that describes the context about this event",
      "type": "string"
    },
    "impression_id": {
      "description": "An alternative UUID used when client_id is absent",
      "pattern": "^\\{[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}\\}$",
      "type": "string"
    },
    "locale": {
      "type": "string"
    },
    "message_id": {
      "type": "string"
    },
    "profile_creation_date": {
      "type": "integer"
    },
    "release_channel": {
      "type": "string"
    },
    "shield_id": {
      "description": "A semicolon separated string to store a list of Shield study IDs",
      "type": "string"
    },
    "version": {
      "type": "string"
    }
  },
  "required": [
    "impression_id",
    "event",
    "addon_version",
    "version",
    "locale"
  ],
  "title": "undesired-events",
  "type": "object"
}
