{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "entries": {
      "description": "A list of log entries.",
      "items": {
        "properties": {
          "details": {
            "description": "Details of the entry.",
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          },
          "timestamp": {
            "description": "Timestamp for the entry.",
            "minimum": 0,
            "type": "integer"
          },
          "url": {
            "description": "The URL the user was focused on.",
            "maxLength": 2048,
            "minLength": 1,
            "type": "string"
          }
        },
        "required": [
          "details",
          "timestamp",
          "url"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": [
    "entries"
  ],
  "title": "pioneer-study-online-news-log",
  "type": "object"
}
