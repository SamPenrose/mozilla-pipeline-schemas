-- -*- lua -*-
filename        = "s3_parquet.lua"
message_matcher = "Type == 'telemetry' && Fields[docType] == 'anonymous'"
ticker_interval = 60
preserve_data   = false

parquet_schema_file = "<%= @schema_path %>/anonymous/anonymous.4.parquetmr.txt"

metadata_group = "metadata"
json_objects = {"Fields[submission]"}
s3_path_dimensions = {
    {name = "submission_date_s3", source = "Fields[submissionDate]"},
}

batch_dir           = "<%= @s3_buffer_dir_disk %>/telemetry-anonymous-parquet/v1"
max_writers         = 100
max_rowgroup_size   = 10000
max_file_size       = 1024 * 1024 * 300
max_file_age        = <%= @max_file_age %>
hive_compatible     = true
