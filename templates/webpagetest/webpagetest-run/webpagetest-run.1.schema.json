{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "webpagetest-run",
  "description": "Web-performance metrics",
  "properties": {
    "appName": {
      "type": "string"
    },
    "appUpdateChannel": {
      "type": "string"
    },
    "connection": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "platform": {
      "type": "string",
      "enum": ["desktop", "mobile"]
    },
    "runner": {
      "type": "string"
    },
    "runId": {
      "type": "integer"
    },
    "sessionState": {
      "type": "string",
      "enum": ["auth", "noAuth"]
    },
    "visualMetrics": {
      "description": "metrics collected by analysing the video (start render, Speed Index, last visual change etc)",
      @WEBPAGETEST_METRICS_MAP_1_JSON@
    },
    "technicalMetrics": {
      "description": "metrics reported by the browser, all the things from navigation timing api, time to first non blank etc",
      @WEBPAGETEST_METRICS_MAP_1_JSON@
    },
    "pageMetrics": {
      "description": "metrics from the page, number of requests, sizes etc",
      @WEBPAGETEST_METRICS_MAP_1_JSON@
    }
  },
  "required": ["appName", "appUpdateChannel", "connection", "domain", "platform", "runner", "runId", "sessionState"],
  "additionalProperties": true
}
